xcopy $(OCC_DIR)3rdparty\freetype-2.5.5-vc11-32\bin\*.dll $(TargetDir) /Y /D
xcopy $(OCC_DIR)3rdparty\freeimage-3.17.0-vc11-32\bin\*.dll $(TargetDir) /Y /D
